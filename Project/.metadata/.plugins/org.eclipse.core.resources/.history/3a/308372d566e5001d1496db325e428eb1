#include "Hex_to_oct.h"

void hex_to_oct(uint16 hexa_no[])
{
	uint16 decimal_number, octa_no = 0;

	decimal_number = HEX_TO_DEC_Trial();
	Dec_to_oct(decimal_number);


}
